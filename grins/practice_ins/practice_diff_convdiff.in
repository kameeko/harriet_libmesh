# Mesh related options
[mesh-options]
mesh_option = 'read_mesh_from_file'
mesh_filename = 'mesh.e'

uniformly_refine = '2'

# Options for time solvers
[unsteady-solver]
transient = false 
theta = 0.5
n_timesteps = 1
deltat = 0.1

#Linear and nonlinear solver options
[linear-nonlinear-solver]
max_nonlinear_iterations = 10 
max_linear_iterations = 2500

initial_linear_tolerance = '1.0e-12'

verify_analytic_jacobians = '1.e-6'

# Visualization options
[vis-options]
output_vis_time_series = 'false'
output_vis = 'true' 
vis_output_file_prefix = 'diff_convdiff'
output_format = 'ExodusII'

# Options for print info to the screen
[screen-options]
print_equation_system_info = 'true' 
print_mesh_info = 'true'
print_log_info = 'true'
solver_verbose = 'true'
solver_quiet = 'false'

echo_physics = 'true'

# [restart-options]
# 
# restart_file = 'stokes_tsection.xdr'

# Options related to all Physics
[Physics]

enabled_physics = 'PracticeDiffusion PracticeConvectionDiffusion'

# Options for diffusion physics
[./PracticeDiffusion]
enabled_subdomains = '2'

fe_family = 'LAGRANGE'
k = '1.0'
source_x = '0.5'
source_y = '-0.1'

bc_ids = '4 5 6 7 8'
bc_types = 'constant_dirichlet constant_dirichlet constant_dirichlet constant_dirichlet constant_dirichlet'

# Options for convection-diffusion physics
[../PracticeConvectionDiffusion]
enabled_subdomains = '1'

fe_family = 'LAGRANGE'
k = '1.0'
source_x = '0.5'
source_y = '-0.1'

bc_ids = '1 2 3'
bc_types = 'constant_dirichlet constant_dirichlet constant_dirichlet'

velocity_file = /home/kameeko/kameeko/course16/grad/dmd/harriet_libmesh/grins/practice_ins/velsT0.txt

