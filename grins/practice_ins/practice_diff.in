# Options related to all Physics
[Physics]

enabled_physics = 'PracticeDiffusion'

[./PracticeDiffusion]

fe_family = 'LAGRANGE'
k = '1.0'

bc_ids = '0 1 2 3'
bc_types = 'constant_dirichlet constant_dirichlet constant_dirichlet constant_dirichlet' #filler

[restart-options]

#restart_file = 'restart.xdr'

[]

# Mesh related options
[mesh-options]
mesh_option = 'create_2D_mesh'
element_type = 'QUAD9'

domain_x1_min = 0.0
domain_x1_max = 1.0
domain_x2_min = 0.0
domain_x2_max = 1.0

mesh_nx1 = 40 
mesh_nx2 = 40

# Options for time solvers
[unsteady-solver]
transient = 'false' 
theta = 1.0 #what is this??
n_timesteps = 1
deltat = 0.0 

#Linear and nonlinear solver options
[linear-nonlinear-solver]
max_nonlinear_iterations = 10 
max_linear_iterations = 2500

verify_analytic_jacobians = 1.0e-6

initial_linear_tolerance = 1.0e-13

use_numerical_jacobians_only = 'false'

require_residual_reduction = 'true'

# Visualization options
[vis-options]
output_vis = 'true'

vis_output_file_prefix = 'practice_diff' 

output_residual = 'false'

output_format = 'ExodusII'

# Options for print info to the screen
[screen-options]

system_name = 'PracticeDiff'

print_equation_system_info = true
print_mesh_info = true
print_log_info = true
solver_verbose = true
solver_quiet = false


print_qoi = 'false'
echo_qoi = 'false'

print_element_jacobians = 'false'

[../VariableNames]

[]


